<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_planes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="0dp"
    tools:context="com.example.andresrugeforero.idvital101116.planes"
    android:background="@android:color/white">

    <Button
        android:text="      Plan ID Vital Travel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/planes4"
        android:drawableRight="@drawable/botonsiguiente"
        android:id="@+id/button8"
        android:textAllCaps="false"
        android:layout_above="@+id/button9"
        android:textAlignment="textStart"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/white" />


    <Button
        android:text="      Plan ID Vital Kids"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/planes3"
        android:drawableRight="@drawable/botonsiguiente"
        android:id="@+id/button9"
        android:textAllCaps="false"
        android:background="@android:color/white"
        android:layout_above="@+id/button11"
        android:layout_alignStart="@+id/button7"
        android:layout_alignParentEnd="true"
        android:textAlignment="viewStart" />

    <Button
        android:text="      Plan ID Vital Pets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/planes2"
        android:drawableRight="@drawable/botonsiguiente"
        android:id="@+id/button11"
        android:textAllCaps="false"
        android:background="@android:color/white"
        android:layout_above="@+id/button10"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:textAlignment="viewStart" />

    <Button
        android:text="      Plan ID Vital Work"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button10"
        android:drawableLeft="@drawable/planes1"
        android:drawableRight="@drawable/botonsiguiente"
        android:textAllCaps="false"
        android:background="@android:color/white"
        android:textAlignment="viewStart"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView2"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="ID Ideal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:gravity="center"
        android:textColorLink="@android:color/holo_green_light"
        tools:textColorHint="@android:color/holo_green_light"
        android:textColor="@android:color/white"
        android:textColorHighlight="@android:color/holo_green_light"
        android:textColorHint="@android:color/holo_green_light"
        android:background="@color/VerdeAdulto"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView2"
        android:layout_alignParentStart="true" />

    <Button
        android:text="      Plan ID Vital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:drawableLeft="@drawable/planes6"
        android:drawableRight="@drawable/botonsiguiente"
        android:textAllCaps="false"
        android:background="@android:color/white"
        android:textAlignment="viewStart"
        android:layout_above="@+id/button7"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="      Plan ID Vital Bike"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:drawableLeft="@drawable/planes5"
        android:drawableRight="@drawable/botonsiguiente"
        android:textAllCaps="false"
        android:background="@android:color/white"
        android:textAlignment="textStart"
        android:layout_above="@+id/button8"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/dosciclas"
        android:id="@+id/imageView4"
        android:layout_above="@+id/button12"
        android:layout_alignParentStart="true" />


</RelativeLayout>


